<idea-plugin>
    <id>com.teleopti.wfm.developer.tools</id>
    <name>Teleopti Wfm Developer Tools</name>
    <version>0.0.6</version>
    <vendor email="fubu@teleopti.com" url="http://www.teleopti.com">Teleopti</vendor>

    <description><![CDATA[
    Stuff and cats.<br>
    ]]></description>

    <change-notes><![CDATA[
    ]]></change-notes>

    <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/build_number_ranges.html for description -->
    <idea-version since-build="171.0"/>

    <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/plugin_compatibility.html
         on how to target different products -->
    <!-- uncomment to enable plugin in all products
    <depends>com.intellij.modules.lang</depends>
    -->
    <depends>com.intellij.modules.lang</depends>

    <extensions defaultExtensionNs="com.intellij">
        <!-- Add your extensions here -->
    </extensions>

    <actions>
        <action
                id="com.teleopti.wfm.developer.tools.actions.FixMyConfigFlow"
                class="com.teleopti.wfm.developer.tools.actions.FixMyConfigFlow"
                text="FixMyConfigFlow"
                description="FixMyConfigFlow"
                icon="/FixMyConfigFlow.png">
            <add-to-group group-id="NavBarToolBar" anchor="last"/>
        </action>
        <action id="com.teleopti.wfm.developer.tools.actions.InfraTestConfig"
                class="com.teleopti.wfm.developer.tools.actions.InfraTestConfig"
                text="InfraTestConfig"
                description="InfraTestConfig"
                icon="/InfraTestConfig.png">
            <add-to-group group-id="NavBarToolBar" anchor="last"/>
        </action>
        <action id="com.teleopti.wfm.developer.tools.actions.GruntNova"
                class="com.teleopti.wfm.developer.tools.actions.GruntNova"
                text="grunt nova"
                description="grunt nova"
                icon="/GruntNova.png">
            <add-to-group group-id="NavBarToolBar" anchor="last"/>
        </action>
        <action id="com.teleopti.wfm.developer.tools.actions.GruntDist"
                class="com.teleopti.wfm.developer.tools.actions.GruntDist"
                text="grunt dist"
                description="grunt dist"
                icon="/GruntDist.png">
            <add-to-group group-id="NavBarToolBar" anchor="last"/>
        </action>
        <action id="com.teleopti.wfm.developer.tools.actions.GruntDevTest"
                class="com.teleopti.wfm.developer.tools.actions.GruntDevTest"
                text="grunt devTest"
                description="grunt devTest"
                icon="/GruntDevTest.png">
            <add-to-group group-id="NavBarToolBar" anchor="last"/>
        </action>
        <action id="com.teleopti.wfm.developer.tools.actions.GruntRtaTest"
                class="com.teleopti.wfm.developer.tools.actions.GruntRtaTest"
                text="grunt rtaTest"
                description="grunt rtaTest"
                icon="/GruntRtaTest.png">
            <add-to-group group-id="NavBarToolBar" anchor="last"/>
        </action>
        <action id="com.teleopti.wfm.developer.tools.actions.CommandLine"
                class="com.teleopti.wfm.developer.tools.actions.CommandLine"
                text="...Web\WFM command line"
                description="...Web\WFM command line"
                icon="/CommandLine.png">
            <add-to-group group-id="NavBarToolBar" anchor="last"/>
        </action>
        <action
                id="com.teleopti.wfm.developer.tools.actions.SuperFlow"
                class="com.teleopti.wfm.developer.tools.actions.SuperFlow"
                text="Super Flow"
                description="Super Flow"
                icon="/SuperFlow.png">
            <add-to-group group-id="NavBarToolBar" anchor="last"/>
        </action>
    </actions>

</idea-plugin>